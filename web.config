<?xml version="1.0" encoding="UTF-8"?>
<configuration>
<system.webServer>
	<rewrite> 
		<rules>
			<rule name="timthumb">     
				<match url="^image/(.*)"/>
				<action type="Rewrite" url="haathumb.php?query={R:1}" appendQueryString="true"/>
			</rule>
		</rules> 
	</rewrite> 
	
	
	</system.webServer>
	
</configuration>